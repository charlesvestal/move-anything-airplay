{
  "id": "airplay",
  "name": "AirPlay",
  "abbrev": "AP",
  "version": "0.1.2",
  "description": "AirPlay audio receiver - stream audio from iPhone, iPad, or Mac",
  "author": "charlesvestal",
  "license": "MIT",
  "ui": "ui.js",
  "ui_chain": "ui.js",
  "dsp": "dsp.so",
  "api_version": 2,
  "capabilities": {
    "audio_out": true,
    "audio_in": false,
    "midi_in": false,
    "midi_out": false,
    "chainable": true,
    "component_type": "sound_generator",
    "chain_params": [
      {
        "key": "gain",
        "name": "Gain",
        "type": "float",
        "min": 0.0,
        "max": 2.0,
        "default": 1.0,
        "step": 0.05
      }
    ]
  },
  "defaults": {
    "gain": 1.0
  }
}
